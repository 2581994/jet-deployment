# [Sonic]
# No configurations

# [PostgreSQL]
POSTGRES_PASSWORD=postgres
POSTGRES_USER=postgres
POSTGRES_DB=postgres
POSTGRES_CONFIG_PATH="/usr/local/etc/postgresql/postgresql.sample.conf"

# [Minio]
MINIO_ACCESS_KEY=minio
MINIO_SECRET_KEY=minio123
MINIO_PRIVATE_BUCKET=minio-private
MINIO_PUBLIC_BUCKET=minio-public
MINIO_REDIS_CONFIG_PATH="/usr/local/etc/redis/redis.sample.conf"

# Notification
MINIO_NOTIFY_REDIS_ENABLE=on
MINIO_NOTIFY_REDIS_FORMAT=access
MINIO_NOTIFY_REDIS_ADDRESS=minioredis:6379
# MINIO_NOTIFY_REDIS_PASSWORD=
MINIO_NOTIFY_REDIS_KEY=minio_events
